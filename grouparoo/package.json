{
  "author": "Grouparoo Inc <hello@grouparoo.com>",
  "name": "@grouparoo/app-example",
  "description": "An example Grouparoo Deployment using a dbt warehouse",
  "version": "0.0.1",
  "license": "MPL-2.0",
  "private": true,
  "engines": {
    "node": ">=12.0.0 <16.0.0"
  },
  "dependencies": {
    "@grouparoo/core": "0.4.1",
    "@grouparoo/dbt": "0.3.3",
    "@grouparoo/mailchimp": "0.3.3",
    "@grouparoo/postgres": "0.3.3",
    "@grouparoo/ui-community": "0.3.3",
    "grouparoo": "0.3.3"
  },
  "scripts": {
    "start": "cd node_modules/@grouparoo/core && ./bin/start",
    "sync": "grouparoo run",
    "test": "grouparoo validate"
  },
  "grouparoo": {
    "plugins": [
      "@grouparoo/dbt",
      "@grouparoo/mailchimp",
      "@grouparoo/postgres",
      "@grouparoo/ui-community"
    ]
  }
}
